  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "postinstall": "husky install",
    "storybook": "start-storybook -p 5000 -s public",
    "build-storybook": "build-storybook -s public",
    "feat": "gh issue create --label feat --project Backlog --assignee sabinmarcu --body \\\"\\\" --title \"$*\" && yarn fetch",
    "bug": "gh issue create --label bug --project Backlog --assignee sabinmarcu --body \\\"\\\" --title \"$*\" && yarn fetch",
    "chore": "gh issue create --label chore --project Backlog --assignee sabinmarcu --body \\\"\\\" --title \"$*\" && yarn fetch",
    "pr": "run-s \"pr:*\"",
    "pr:push": "git push origin \"$(yarn config:branch)\"",
    "pr:create": "gh pr create --base staging --repo \"$(yarn config:remote)\" --head \"$(yarn config:branch)\" --project Backlog --fill",
    "pr:merge": "gh pr merge -rd",
    "pr:pull": "git pull origin \"$(yarn config:branch)\"",
    "pr:prune": "yarn prune",
    "pr:perm": "yarn config:perm",
    "prune": "run-s \"prune:*\"",
    "prune:local": "git prune",
    "prune:remote": "git remote prune origin",
    "config:remote": "git config --get remote.origin.url | cut -d: -f2- | cut -d. -f1",
    "config:branch": "git rev-parse --abbrev-ref HEAD",
    "config:issue": "yarn config:branch | cut -d/ -f2- | cut -d- -f2-",
    "config:perm": "find .husky -type f -print0 | xargs -0 chmod +x"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@mdx-js/react": "^1.6.22",
    "@storybook/addon-actions": "^6.1.11",
    "@storybook/addon-essentials": "^6.1.11",
    "@storybook/addon-links": "^6.1.11",
    "@storybook/addons": "^6.1.11",
    "@storybook/node-logger": "^6.1.11",
    "@storybook/preset-create-react-app": "^3.1.5",
    "@storybook/react": "^6.1.11",
    "@storybook/theming": "^6.1.11",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/color": "^3",
    "@types/jest": "^26.0.15",
    "@types/mdx-js__react": "^1",
    "@types/node": "^14.14.14",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-measure": "^2",
    "@types/react-router": "^5",
    "@types/webpack": "^4.41.25",
    "@types/webpack-env": "^1.16.0",
    "@typescript-eslint/eslint-plugin": "^4.10.0",
    "eslint": "^7.16.0",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "eslint-import-resolver-node": "^0.3.4",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "next",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
